<html class='rollPageR'>
<table id = 'rolltable' class='RollBox'>
    <tr>
        <td>
          <input type='button'id='D1' onclick='clickme(this);' value='<%= @rollArray[0] %>'/>
        </td>
        <td>
            <input type='button'id='D2' onclick='clickme(this);' value='<%= @rollArray[1] %>'/>
        </td>
    </tr>
    <tr>
        <td>
           <input type='button'id='D3' onclick='clickme(this);' value='<%= @rollArray[2] %>'/>
        </td>
        <td>
            <input type='button'id='D4' onclick='clickme(this);' value='<%= @rollArray[3] %>'/>
        </td>
    </tr>
    <tr>
        <td>
           <input type='button'id='D5' onclick='clickme(this);' value='<%= @rollArray[4] %>'/>
        </td>
