        </td>
    </tr>
</table>

<script>
   var used = [];
   window.test='a';
    function clickme(pid)
    {
        if(window.isSelected == 1) {
        var pele = document.getElementById(window.selectedID);
        pele.setAttribute('class','');
        }
        var ele = pid;//document.getElementById(pid);
        parent.parent.selectedValue = ele.value;
        parent.parent.selectedID = ele.id;
        parent.parent.isSelected = 1;
        ele.setAttribute('class','clicked');
    }
    function unClick(pid)
    {
        var ele = document.getElementById(pid);
        ele.setAttribute('class','dclicked');
    }
</script>
</html>